{
  "name": "@apollo/library",
  "description": "Apollo component library",
  "version": "2.47.0",
  "author": "Apollo Team <apollo@finfactory.com>",
  "private": true,
  "scripts": {
    "dll:dev": "NODE_ENV=development webpack --config ./webpack.dll.config.js",
    "dll:prod": "NODE_ENV=production webpack --config ./webpack.dll.config.js",
    "build:dll": "npm run dll:dev && npm run dll:prod",
    "analyse": "npm outdated && npm prune && du -hsc ./node_modules",
    "doc": "esdoc -c esdoc.json",
    "test": "mkdir -p ./locale/messages && NODE_ENV=development karma start",
    "test:ci": "mkdir -p ./locale/messages && NODE_ENV=development karma start --singleRun=true",
    "test:watch": "mkdir -p ./locale/messages && NODE_ENV=development karma start --singleRun false --reporters mocha,notify"
  },
  "repository": {
    "type": "git",
    "url": "git@git.finfactory.com:fe_library.git"
  },
  "engines": {
    "node": "7.x.x",
    "npm": "3.x.x"
  },
