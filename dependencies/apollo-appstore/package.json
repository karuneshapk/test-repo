    "@apollo/library": "file:./apollo-library",
    "babel-eslint": "^7.1.0",
    "eslint": "^3.9.1",
    "eslint-config-apollo": "file:./apollo-eslint-rules",
    "eslint-plugin-react": "^6.6.0",
