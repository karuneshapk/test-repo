{
  "source": "./src",
  "destination": "./doc",
  "autoPrivate": true,
  "includes": ["\\.(js|jsx)$"],
  "excludes": ["fetch\\.js$"],
  "access": ["public", "protected"],
  "package": "./package.json",
  "coverage": true,
  "includeSource": true,
  "lint": true,
  "plugins": [
    {"name": "esdoc-es7-plugin"}
  ]
}